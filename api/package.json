		"@types/compression": "^1.7.2",
		"@types/connect-redis": "^0.0.18",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/express-fileupload": "^1.2.2",
		"@types/express-session": "^1.17.4",
		"@types/node": "^17.0.21",
		"@types/uuid": "^8.3.4",
		"@types/ws": "^8.5.0",
		"nodemon": "^2.0.15",
		"ts-node": "^10.5.0",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"@types/dotenv": "^8.2.0",
		"@types/redis": "^4.0.11",
		"compression": "^1.7.4",
