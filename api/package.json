{
	"name": "file-processor-api",
	"version": "0.1.0",
	"description": "File Processor, (nodejs) api server",
	"main": "index.js",
	"scripts": {
		"dotfile": "export $(cat ../.dotfiles/.prod",
		"serve": "export $(cat ../.env | xargs) && node prod/index.js",
